
@using  webapp.Utilities

<html lang=en>
<head>
    <meta charset=utf-8>
    <meta http-equiv=X-UA-Compatible content="IE=edge">
    <meta name=viewport content="width=device-width, initial-scale=1">
    <meta name=description content="">
    <meta name=author content="">
     
    <link rel="shortcut icon" href="~/assets/dist/img/ico/favicon.png" type="image/x-icon">
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
    <script>
        WebFont.load({
            google: {
                families: ['Alegreya+Sans:100,100i,300,300i,400,400i,500,500i,700,700i,800,800i,900,900i', 'Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i', 'Open Sans']
            }
        });
    </script>
    <link href="~/assets/plugins/toastr/toastr.min.css" rel=stylesheet type="text/css" />
    <link href="~/assets/dist/css/base.css" rel=stylesheet type="text/css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/assets/dist/css/component_ui.min.css" rel=stylesheet type="text/css" />
    <link id=defaultTheme href="~/assets/dist/css/skins/skin-blue.min.css" rel=stylesheet type="text/css" />
    <link href="~/assets/dist/css/custom.css" rel=stylesheet type="text/css" />

    <link href="~/assets/css/templatemo-style.css" rel="stylesheet" />

</head>
<body>
    <div class="row">
        <div class="col-sm-12">
            <div class="tm-sidebar">
                <nav class="tm-main-nav">
                    <ul class="tm-main-nav-ul">
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "Home")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-home tm-nav-fa"></i>Home
                            </a>
                        </li>
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "Fileupload")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-sitemap tm-nav-fa"></i>Documents upload
                            </a>
                        </li>
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "Order")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-sitemap tm-nav-fa"></i>Order Report
                            </a>
                        </li>
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "Production")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-sitemap tm-nav-fa"></i>Production Report
                            </a>
                        </li>
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "Invoice")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-sitemap tm-nav-fa"></i>Invoice Report
                            </a>
                        </li>
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "DateProduction")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-sitemap tm-nav-fa"></i>Receipt Detail
                            </a>
                        </li>
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "Issue")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-sitemap tm-nav-fa"></i>Issue Detail
                            </a>
                        </li>
                        <li class="tm-nav-item">
                            <a href="@Url.Action("Index", "Summary")" class="tm-nav-item-link tm-button">
                                <i class="fa fa-sitemap tm-nav-fa"></i>Summary
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>

            <div class="tm-main-content">
                @RenderBody()
            </div>
        </div>
    </div>
    <script src="~/assets/plugins/jQuery/jquery-1.12.4.min.js"></script>
    <script src="~/assets/plugins/jquery-ui-1.12.1/jquery-ui.min.js"></script>
    <script src="~/assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/assets/plugins/metisMenu/metisMenu.min.js"></script>
    <script src="~/assets/plugins/lobipanel/lobipanel.min.js"></script>
    <script src="~/assets/plugins/animsition/js/animsition.min.js"></script>
    <script src="~/assets/plugins/datatables/dataTables.min.js" type="text/javascript"></script>
    <link href="~/assets/plugins/datatables/dataTables.min.css" rel="stylesheet" type="text/css" />
